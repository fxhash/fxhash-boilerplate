{
  "name": "fxhash-boilerplate",
  "private": true,
  "version": "2.0.0",
  "description": "The fxhash boilerplate, comes with some utilities to create a project for fxhash",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "postinstall": "node ./lib/scripts/install.js",
    "start": "node ./lib/scripts/start.js",
    "start:project": "webpack serve --config ./lib/config/webpack.config.dev.js",
    "build": "webpack --config ./lib/config/webpack.config.prod.js"
  },
  "author": "fxhash",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "20.0.1",
    "adm-zip": "0.5.10",
    "axios": "1.4.0",
    "boxen": "7.1.1",
    "chalk": "4.1.2",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "github-download-directory": "3.0.0",
    "html-webpack-plugin": "5.5.3",
    "style-loader": "3.3.3",
    "webpack": "5.88.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.1"
  },
  "devDependencies": {
    "@types/adm-zip": "0.5.0",
    "@types/chalk": "2.2.0",
    "@types/express": "4.17.17",
    "keyv": "4.5.3",
    "yesno": "0.4.0"
  }
}
